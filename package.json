{
  "name": "lirest_be",
  "version": "0.1.0",
  "description": "Book Libary REST API",
  "private": true,
  "main": "src/main.ts",
  "engines": {
    "node": "16.x",
    "pnpm": "7.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install && husky add .husky/pre-commit \"lint-staged\"",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register .",
    "start:dev": "tsnd --respawn --clear --transpile-only -r tsconfig-paths/register . --ignore-watch \"src/**/*.spec.ts\" node_modules"
  },
  "author": "Tran Lam",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^6.5.1",
    "express-validator": "^6.14.0",
    "express-winston": "^4.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "ms": "^2.1.3",
    "mysql2": "^2.3.3",
    "openapi-types": "^11.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "triple-beam": "^1.3.0",
    "tsconfig-paths": "^3.14.1",
    "typeorm": "^0.3.6",
    "winston": "^3.7.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.30",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
